<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="500.0" prefWidth="700.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sbc.diagnocom.SE2FController">
   <children>
      <AnchorPane prefHeight="500.0" prefWidth="700.0">
         <children>
            <ImageView fitHeight="500.0" fitWidth="800.0" />
            <Button fx:id="encadenar" layoutX="136.0" layoutY="436.0" mnemonicParsing="false" onAction="#encadenarPressed" text="RESULTADO" />
            <Button fx:id="next" layoutX="240.0" layoutY="436.0" mnemonicParsing="false" onAction="#nextPressed" text="SIGUIENTE" />
            <Button fx:id="salir" layoutX="538.0" layoutY="436.0" mnemonicParsing="false" onAction="#salirPressed" text="SALIR" />
            <Label fx:id="resultado" layoutX="418.0" layoutY="260.0" prefHeight="77.0" prefWidth="287.0" text="RESULTADO" textAlignment="JUSTIFY" wrapText="true" />
            <Button fx:id="reiniciar" layoutX="341.0" layoutY="436.0" mnemonicParsing="false" onAction="#reiniciarPressed" text="REINICIAR" />
            <CheckBox fx:id="dolorCabeza" layoutX="88.0" layoutY="101.0" mnemonicParsing="false" text="¿Tiene dolor de cabeza?" />
            <CheckBox fx:id="presionArtAlta" layoutX="88.0" layoutY="144.0" mnemonicParsing="false" text="¿Tiene presión arterial alta?" />
            <CheckBox fx:id="fiebre" layoutX="88.0" layoutY="189.0" mnemonicParsing="false" text="¿Tiene fiebre?" />
            <CheckBox fx:id="zumbidoOidos" layoutX="88.0" layoutY="233.0" mnemonicParsing="false" text="¿Tiene zumbido de oidos?" />
            <CheckBox fx:id="nauseas" layoutX="88.0" layoutY="277.0" mnemonicParsing="false" text="¿Tiene nauseas?" />
            <CheckBox fx:id="dolorGarganta" layoutX="88.0" layoutY="319.0" mnemonicParsing="false" text="¿Tiene dolor de garganta?" />
            <CheckBox fx:id="dolorOido" layoutX="88.0" layoutY="369.0" mnemonicParsing="false" text="¿Tiene dolor de oido?" />
            <Label layoutX="391.0" layoutY="31.0" text="AFECCIÓN DE CABEZA" />
            <Label fx:id="info" layoutX="408.0" layoutY="134.0" prefHeight="77.0" prefWidth="287.0" text="INFO" textAlignment="JUSTIFY" wrapText="true" />
            <Button fx:id="limpiarCeldas" layoutX="444.0" layoutY="436.0" mnemonicParsing="false" onAction="#limpiarCeldasPressed" text="LIMPIAR" />
         </children>
      </AnchorPane>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
