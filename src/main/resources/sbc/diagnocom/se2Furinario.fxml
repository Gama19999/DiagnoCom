<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="500.0" prefWidth="700.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sbc.diagnocom.SE2FController">
   <children>
      <AnchorPane prefHeight="500.0" prefWidth="700.0">
         <children>
            <ImageView fitHeight="500.0" fitWidth="800.0" />
            <Button fx:id="showResult" layoutX="136.0" layoutY="436.0" mnemonicParsing="false" onAction="#encadenar" text="RESULTADO" />
            <Button fx:id="next" layoutX="240.0" layoutY="436.0" mnemonicParsing="false" onAction="#nextPressed" text="SIGUIENTE" />
            <Button fx:id="salir" layoutX="446.0" layoutY="436.0" mnemonicParsing="false" onAction="#salirPressed" text="SALIR" />
            <Label fx:id="resultado" layoutX="418.0" layoutY="260.0" text="RESULTADO" />
            <Button fx:id="reiniciar" layoutX="341.0" layoutY="436.0" mnemonicParsing="false" onAction="#reiniciarPressed" text="REINICIAR" />
         </children>
      </AnchorPane>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
