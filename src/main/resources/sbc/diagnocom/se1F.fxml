<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="500.0" prefWidth="700.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sbc.diagnocom.SE1FController">
   <children>
      <AnchorPane prefHeight="500.0" prefWidth="700.0">
         <children>
            <ImageView fitHeight="500.0" fitWidth="800.0" />
            <CheckBox fx:id="afeccionCabeza" layoutX="124.0" layoutY="103.0" mnemonicParsing="false" text="¿Molestia en la cabeza?" />
            <CheckBox fx:id="afeccionRespiratoria" layoutX="126.0" layoutY="151.0" mnemonicParsing="false" text="¿Molestia respiratoria?" />
            <CheckBox fx:id="afeccionDigestiva" layoutX="126.0" layoutY="199.0" mnemonicParsing="false" text="¿Molestia digestiva?" />
            <CheckBox fx:id="afeccionInterna" layoutX="126.0" layoutY="250.0" mnemonicParsing="false" text="¿Molestia interna?" />
            <CheckBox fx:id="afeccionUrinaria" layoutX="126.0" layoutY="299.0" mnemonicParsing="false" text="¿Molestia urinaria?" />
            <CheckBox fx:id="afeccionPiel" layoutX="126.0" layoutY="348.0" mnemonicParsing="false" text="¿Molestia en la piel?" />
            <Button fx:id="encadenar" layoutX="126.0" layoutY="426.0" mnemonicParsing="false" onAction="#encadenarPressed" text="RESULTADO" />
            <Button fx:id="next" layoutX="230.0" layoutY="426.0" mnemonicParsing="false" onAction="#nextPressed" text="SIGUIENTE" />
            <Button fx:id="salir" layoutX="436.0" layoutY="426.0" mnemonicParsing="false" onAction="#salirPressed" text="SALIR" />
            <Label fx:id="resultado" layoutX="408.0" layoutY="250.0" prefHeight="77.0" prefWidth="287.0" text="RESULTADO" textAlignment="JUSTIFY" wrapText="true" />
            <Button fx:id="limpiarCeldas" layoutX="336.0" layoutY="426.0" mnemonicParsing="false" onAction="#limpiarCeldasPressed" text="LIMPIAR" />
            <Label layoutX="376.0" layoutY="26.0" text="LUGAR DE LA AFECCIÓN" />
            <Label fx:id="info" layoutX="408.0" layoutY="134.0" prefHeight="77.0" prefWidth="287.0" text="INFO" textAlignment="JUSTIFY" wrapText="true" />
         </children>
      </AnchorPane>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
