<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="500.0" prefWidth="700.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sbc.diagnocom.SE1FController">
   <children>
      <AnchorPane prefHeight="500.0" prefWidth="700.0">
         <children>
            <ImageView fitHeight="500.0" fitWidth="800.0" />
            <CheckBox fx:id="afeccionCabeza" layoutX="126.0" layoutY="104.0" mnemonicParsing="false" text="¿Molestia en la cabeza?" />
            <CheckBox fx:id="afeccionRespiratoria" layoutX="126.0" layoutY="151.0" mnemonicParsing="false" text="¿Molestia respiratoria?" />
            <CheckBox fx:id="afeccionDigestiva" layoutX="126.0" layoutY="197.0" mnemonicParsing="false" text="¿Molestia digestiva?" />
            <CheckBox fx:id="afeccionInterna" layoutX="126.0" layoutY="250.0" mnemonicParsing="false" text="¿Molestia interna?" />
            <CheckBox fx:id="afeccionUrinaria" layoutX="126.0" layoutY="299.0" mnemonicParsing="false" text="¿Molestia urinaria?" />
            <CheckBox fx:id="afeccionPiel" layoutX="126.0" layoutY="348.0" mnemonicParsing="false" text="¿Molestia en la piel?" />
            <Button fx:id="showResult" layoutX="126.0" layoutY="426.0" mnemonicParsing="false" onAction="#encadenar" text="RESULTADO" />
            <Button fx:id="next" layoutX="230.0" layoutY="426.0" mnemonicParsing="false" onAction="#nextPressed" text="SIGUIENTE" />
            <Button fx:id="salir" layoutX="436.0" layoutY="426.0" mnemonicParsing="false" onAction="#salirPressed" text="SALIR" />
            <Label fx:id="resultado" layoutX="408.0" layoutY="250.0" text="RESULTADO" />
            <Button fx:id="reiniciar" layoutX="331.0" layoutY="426.0" mnemonicParsing="false" onAction="#reiniciarPressed" text="REINICIAR" />
         </children>
      </AnchorPane>
   </children>
</VBox>
