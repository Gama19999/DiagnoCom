<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="500.0" prefWidth="700.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sbc.diagnocom.SE1FController">
   <children>
      <AnchorPane prefHeight="500.0" prefWidth="700.0">
         <children>
            <ImageView fitHeight="500.0" fitWidth="800.0">
               <image>
                  <Image url="@../images/fondo_oficial.png" />
               </image></ImageView>
            <CheckBox fx:id="afeccionCabeza" layoutX="85.0" layoutY="103.0" mnemonicParsing="false" style="-fx-font-family: roboto; -fx-font-weight: bold; -fx-font-size: 16; -fx-font-style: italic;" text="¿Molestia en la cabeza?" />
            <CheckBox fx:id="afeccionRespiratoria" layoutX="85.0" layoutY="151.0" mnemonicParsing="false" style="-fx-font-family: roboto; -fx-font-weight: bold; -fx-font-size: 16; -fx-font-style: italic;" text="¿Molestia respiratoria?" />
            <CheckBox fx:id="afeccionDigestiva" layoutX="85.0" layoutY="199.0" mnemonicParsing="false" style="-fx-font-family: roboto; -fx-font-weight: bold; -fx-font-size: 16; -fx-font-style: italic;" text="¿Molestia digestiva?" />
            <CheckBox fx:id="afeccionInterna" layoutX="85.0" layoutY="250.0" mnemonicParsing="false" style="-fx-font-family: roboto; -fx-font-weight: bold; -fx-font-size: 16; -fx-font-style: italic;" text="¿Molestia interna?" />
            <CheckBox fx:id="afeccionUrinaria" layoutX="85.0" layoutY="299.0" mnemonicParsing="false" style="-fx-font-family: roboto; -fx-font-weight: bold; -fx-font-size: 16; -fx-font-style: italic;" text="¿Molestia urinaria?" />
            <CheckBox fx:id="afeccionPiel" layoutX="85.0" layoutY="348.0" mnemonicParsing="false" style="-fx-font-family: roboto; -fx-font-weight: bold; -fx-font-size: 16; -fx-font-style: italic;" text="¿Molestia en la piel?" />
            <Button fx:id="encadenar" layoutX="20.0" layoutY="464.0" mnemonicParsing="false" onAction="#encadenarPressed" prefWidth="130.0" style="-fx-border-color: #3a76a1; -fx-border-radius: 15; -fx-border-width: 2; -fx-background-color: none;" text="RESULTADO">
               <font>
                  <Font name="Gabriola" size="19.0" />
               </font>
               <padding>
                  <Insets top="-0.5" />
               </padding></Button>
            <Button fx:id="next" layoutX="187.0" layoutY="464.0" mnemonicParsing="false" onAction="#nextPressed" prefWidth="130.0" style="-fx-border-color: #3a76a1; -fx-border-radius: 15; -fx-border-width: 2; -fx-background-color: none;" text="SIGUIENTE">
               <font>
                  <Font name="Gabriola" size="19.0" />
               </font>
               <padding>
                  <Insets top="-0.5" />
               </padding></Button>
            <Button fx:id="salir" layoutX="656.0" layoutY="464.0" mnemonicParsing="false" onAction="#salirPressed" prefWidth="130.0" style="-fx-border-color: #3a76a1; -fx-border-radius: 15; -fx-border-width: 2; -fx-background-color: none;" text="SALIR">
               <font>
                  <Font name="Gabriola" size="19.0" />
               </font>
               <padding>
                  <Insets top="-0.5" />
               </padding></Button>
            <Label fx:id="resultado" layoutX="440.0" layoutY="219.0" prefHeight="77.0" prefWidth="287.0" style="-fx-font-family: &quot;Comic Sans MS&quot;; -fx-font-weight: bold;" text="RESULTADO" wrapText="true" />
            <Button fx:id="limpiarCeldas" layoutX="490.0" layoutY="464.0" mnemonicParsing="false" onAction="#limpiarCeldasPressed" prefWidth="130.0" style="-fx-border-color: #3a76a1; -fx-border-radius: 15; -fx-border-width: 2; -fx-background-color: none;" text="LIMPIAR">
               <font>
                  <Font name="Gabriola" size="19.0" />
               </font>
               <padding>
                  <Insets top="-0.5" />
               </padding></Button>
            <Label layoutX="327.0" layoutY="14.0" style="-fx-font-family: serif; -fx-font-size: 25; -fx-font-weight: bold;" text="LUGAR DE LA AFECCIÓN" />
            <Label fx:id="info" layoutX="440.0" layoutY="103.0" prefHeight="77.0" prefWidth="287.0" style="-fx-font-family: &quot;Comic Sans MS&quot;; -fx-font-weight: bold;" text="INFO" wrapText="true" />
         </children>
      </AnchorPane>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
